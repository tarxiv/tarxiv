# Supabase Configuration
# Copy this to your .env file and adjust the values

# Database
SUPABASE_DB_NAME=postgres
SUPABASE_DB_USER=postgres
SUPABASE_DB_PASSWORD=your-super-secret-password
SUPABASE_DB_DATA_DIR=.data/supabase/db

# JWT Secret - MUST be at least 32 characters long
# Generate a secure one with: openssl rand -base64 32
SUPABASE_JWT_SECRET=your-super-secret-jwt-token-with-at-least-32-characters-long

# API Configuration
SUPABASE_API_EXTERNAL_URL=http://localhost:8000
SUPABASE_SITE_URL=http://localhost:3000
SUPABASE_URI_ALLOW_LIST=*

# Auth Configuration
SUPABASE_DISABLE_SIGNUP=false
SUPABASE_EXTERNAL_EMAIL_ENABLED=true
SUPABASE_MAILER_AUTOCONFIRM=true

# SMTP Configuration (optional - for sending emails)
# Leave empty to auto-confirm users
SUPABASE_SMTP_HOST=
SUPABASE_SMTP_PORT=587
SUPABASE_SMTP_USER=
SUPABASE_SMTP_PASS=
SUPABASE_SMTP_ADMIN_EMAIL=admin@example.com

